<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test2">

    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LasPlot"
            android:label="@string/title_activity_las_plot"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".OpenLasPage"
            android:label="@string/title_activity_open_las_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HelpPage"
            android:label="@string/title_activity_help_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LandingPage" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>